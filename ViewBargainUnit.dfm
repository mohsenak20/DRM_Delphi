  object QSummeryCard: TADOQuery
    Connection = DM.ADOConnection
    Parameters = <>
    SQL.Strings = (
      
        'Declare @SalesCard int, @UsedCard int, @NotUsedCard int, @Specia' +
        'lCard int, @UsedSpecialCard int, @NotUsedSpecialCard int'
      
        'Declare @NormalSalesCard int, @NormalUsedCard int, @NormalNotUse' +
        'dCard int'
      ''
      ''
      'set @SalesCard=(select Count(CardID) from TCard)'
      ' '
      
        'set @UsedCard=((select COUNT(ReceptionID) from TReception where ' +
        'CardID>4)+'
      '(select COUNT(ReceptionID) from TReception where CardID2>4)+'
      '(select COUNT(ReceptionID) from TReception where CardID3>4))'
      ''
      'set @NotUsedCard = @SalesCard - @UsedCard'
      ''
      
        'set @SpecialCard = (select Count(CardID) from TBargain inner joi' +
        'n TCard on TBargain.BargainID = TCard.BargainID where GetMoney =' +
        ' 1) + '
      
        '(select COUNT(CardID) from TBargain inner join TCard on TBargain' +
        '.BargainID = TCard.BargainID where CompanyID = 5 or CompanyID = ' +
        '245)'
      ''
      ''
      
        'set @UsedSpecialCard = (select Count(ReceptionID) from TBargain ' +
        'inner join TCard on TBargain.BargainID = TCard.BargainID '
      
        'inner join TReception on TCard.CardID=TReception.CardID where Ge' +
        'tMoney = 1) + '
      
        '(select COUNT(ReceptionID) from TBargain inner join TCard on TBa' +
        'rgain.BargainID = TCard.BargainID '
      
        'inner join TReception on TCard.CardID=TReception.CardID where Co' +
        'mpanyID = 5 or CompanyID = 245)+'
      ''
      
        '(select Count(ReceptionID) from TBargain inner join TCard on TBa' +
        'rgain.BargainID = TCard.BargainID '
      
        'inner join TReception on TCard.CardID=TReception.CardID2 where G' +
        'etMoney = 1) + '
      
        '(select COUNT(ReceptionID) from TBargain inner join TCard on TBa' +
        'rgain.BargainID = TCard.BargainID '
      
        'inner join TReception on TCard.CardID=TReception.CardID2 where C' +
        'ompanyID = 5 or CompanyID = 245)+'
      ''
      
        '(select Count(ReceptionID) from TBargain inner join TCard on TBa' +
        'rgain.BargainID = TCard.BargainID '
      
        'inner join TReception on TCard.CardID=TReception.CardID3 where G' +
