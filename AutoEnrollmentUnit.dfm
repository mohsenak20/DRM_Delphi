  object QTermCount: TADOQuery
    Connection = DM.ADOConnection
    Parameters = <
      item
        Name = 'termId'
        Attributes = [paSigned]
        DataType = ftInteger
        Precision = 10
        Size = 4
        Value = Null
      end>
    SQL.Strings = (
      'select count(Tstudent.id) count from TStudent'
      
        'inner join TStudentEnrollment on TStudent.id = TStudentEnrollmen' +
        't.studentId'
      
        'where isSuspend = 0 and enrollNewTerm = 1 and TStudentEnrollment' +
        '.termId = :termId')
    Left = 24
