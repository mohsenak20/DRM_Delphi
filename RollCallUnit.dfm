    SQL.Strings = (
      
        'select TRollCall.*, TStudent.name+'#39' '#39'+TStudent.family name, mobi' +
        'le, NID, fatherName ,'
      'TTerm.title termTitle'
      'from TRollCall '
      'inner join TStudent on TRollCall.studentId = TStudent.id'
      
        'inner join TTermSchedule on TRollCall.termScheduleId = TTermSche' +
        'dule.id'
      'inner join TTerm on TTermSchedule.termId = TTerm.id'
      'where termScheduleId = :termScheduleId'
      'order by id DESC')
