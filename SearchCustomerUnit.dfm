  object QManagerCard: TADOQuery
    Connection = DM.ADOConnection
    Parameters = <
      item
        Name = 'ReceptionID'
        Size = -1
        Value = Null
      end>
    SQL.Strings = (
      
        'Declare @CompanyID1 int, @CompanyID2 int, @CompanyID3 int, @Rece' +
        'ptionID bigint'
      
        'Declare @CompanyName1 nvarchar(50), @CompanyName2 nvarchar(50), ' +
        '@CompanyName3 nvarchar(50)'
      'set @ReceptionID = :ReceptionID'
      'Declare @CardID1 Bigint, @CardID2 Bigint, @CardID3 Bigint'
      ''
      
        'select @CardID1=CardID, @CardID2=CardID2, @CardID3=CardID3 from ' +
        'TReception'
      'where ReceptionID = @ReceptionID'
      ''
      
        'select @CompanyID1=TCompany.CompanyID, @CompanyName1=CompanyName' +
        ' from TBargain inner join TCard'
      
        'on TBargain.BargainID = TCard.BargainID inner join TCompany on T' +
        'Bargain.CompanyID = TCompany.CompanyID'
      'where CardID = @CardID1'
      ''
      
        'select @CompanyID2=TCompany.CompanyID, @CompanyName2=CompanyName' +
        ' from TBargain inner join TCard'
      
        'on TBargain.BargainID = TCard.BargainID inner join TCompany on T' +
        'Bargain.CompanyID = TCompany.CompanyID'
      'where CardID = @CardID2'
      ''
      ''
      
        'select @CompanyID3=TCompany.CompanyID, @CompanyName3=CompanyName' +
        ' from TBargain inner join TCard'
      
        'on TBargain.BargainID = TCard.BargainID inner join TCompany on T' +
        'Bargain.CompanyID = TCompany.CompanyID'
      'where CardID = @CardID3'
      ''
      'select @CompanyID1 CompanyID1, @CompanyName1 CompanyName1, '
      '@CompanyID2 CompanyID2, @CompanyName2 CompanyName2, '
      '@CompanyID3 CompanyID3, @CompanyName3 CompanyName3'
      ''
      ''
      '')
    Left = 184
    Top = 29
  end
