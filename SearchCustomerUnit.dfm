  object QSearch: TADOQuery
    Connection = DM.ADOConnection
    Parameters = <
      item
        Name = 'UserID'
        Size = -1
        Value = Null
      end
      item
        Name = 'KeyNumber'
        Attributes = [paSigned]
        DataType = ftInteger
        Precision = 10
        Size = 4
        Value = Null
      end>
    SQL.Strings = (
      'declare @service_center_id int'
      
        'select @service_center_id = service_center_id from TUser where U' +
        'serID = :UserID'
      'Select * from TReception inner join TClosed on'
      'TReception.ClosedID = TClosed.ClosedID'
      
        'where KeyNumber = :KeyNumber and EndTime='#39'99:99'#39' and TReception.' +
        'service_center_id = @service_center_id')
    Left = 184
    Top = 69
  end
